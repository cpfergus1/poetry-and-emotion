<h1> Search Results </h1>
<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Poem</th>
    <th>Tone</th>
    <th>Tone Score</th>
  </tr>
  <%@poems.each do |poem|%>
    <tr class='Poem'>
      <td> <%= poem.title %> </td>
      <td><%= poem.author %></td>
      <td><%= poem.lines %></td>
      <td><%= poem.tone.keys.join(', ') %></td>
      <td><%= poem.tone.values.join(', ') %></td>
    </tr>
  <%end%>
</table>
